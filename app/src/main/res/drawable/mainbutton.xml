<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="2000dp" />
            <solid android:color="#009688" />
            <stroke
                android:width="4dip"
                android:color="#F0F1F6" />
            <padding
                android:bottom="4dp"
                android:left="4dp"
                android:right="4dp"
                android:top="4dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:layout_height="match_parent" android:shape="rectangle">
            <corners android:radius="1200dp" />
            <solid android:color="#1633C8" />
            <stroke android:width="8dip" android:color="#1633C8" />
            <padding android:bottom="6dp" android:left="6dp" android:right="6dp" android:top="6dp" />
        </shape>
    </item>
</selector>